########### BACKUP SCHEDULE
BACKUP_CRON_EXPRESSION="@daily"
# BACKUP_COMPRESSION="gz"
# GZIP_PARALLELISM="1"
BACKUP_FILENAME="$USERNAME-%Y-%m-%dT%H-%M-%S.{{ .Extension }}"
BACKUP_FILENAME_EXPAND="true"
BACKUP_SOURCES="/backup"
BACKUP_EXCLUDE_REGEXP="\.log$"

